{
  "name": "find-duplicate-ids",
  "version": "1.6.1",
  "private": true,
  "description": "GitHub Action to search for duplicate HTML IDs in markdown files",
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint",
    "fix": "npm run lint -- --fix",
    "format": "prettier --write '**/*.ts'",
    "clean": "rimraf lib dist",
    "compile": "tsc",
    "package": "ncc build ./lib/main.js -o dist --source-map --license licenses.txt",
    "pretest": "npm run lint",
    "test": "jest",
    "all": "npm run clean && npm run compile && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UCLATALL/actions.git",
    "directory": "find-duplicate-ids"
  },
  "keywords": [
    "coursekata",
    "actions",
    "validate",
    "markdown",
    "duplicates"
  ],
  "author": "Adam Blake",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/glob": "^0.4.0",
    "dedent": "^1.5.0",
    "jsdom": "^24.0.0",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.4"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.5.1",
    "@types/jsdom": "^21.1.6",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/node": "^17.0.35",
    "@typescript-eslint/parser": "^5.26.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^8.16.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.2.2",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
